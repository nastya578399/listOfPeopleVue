CAAA,KAAA,GAAK,IAAP;AAAA,KAAA;AAA4B,GAA5D,E,IAAA,E;;AAAA,I,SAAA,CAuBI,EArBJ,YAAA,CAWO,MAXP,EAWO;AAXD,aAAM,MAWL;AAXa,IAAA,QAAM,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,cAAA,CAAA;AAAA,aAAU,QAAA,CAAA,YAAA,OAAA,QAAA,YAAV;AAAA,KAAA,EAAsB,CAAA,SAAA,CAAtB,CAAA;AAWnB,GAXP,E,CACE,U,EAEA,YAAA,CAGM,KAHN,EAAA,UAAA,EAGM,CAFJ,UAEI,E,gBADJ,YAAA,CAAiD,OAAjD,EAAiD;AAA1C,IAAA,IAAI,EAAC,MAAqC;AAA9B,IAAA,EAAE,EAAC,MAA2B;;aAAN,KAAA,CAAA,IAAA,GAAI,M;;AAAE,GAAjD,E,IAAA,E;;AAAA,G,iBAA2C,KAAA,CAAA,I;UAAd;MACzB,CAHN,C,EAKA,YAAA,CAES,QAFT,EAES;AAFD,aAAM,aAEL;AAFoB,IAAA,QAAQ,EAAE,KAAA,CAAA,IAAA,CAAK,MAAL,KAAW;AAEzC,GAFT,EAA0D,oBAA1D,EAEA;AAAA;AAFA,IAEA,CAAA,UAAA,CAFA,C,CARF,E;;AAAA,GAqBI,EARc,KAAA,CAAA,O,kBAAlB,YAAA,CAAwC,qBAAxC,EAAwC;AAAA,IAAA,GAAA,EAAA;AAAA,GAAxC,C,mBAEA,YAAA,CAKmB,0BALnB,EAKmB;UAAA;AAHlB,IAAA,MAAM,EAAE,KAAA,CAAA,MAGU;AAFlB,IAAA,MAAI,EAAE,QAAA,CAAA,UAEY;AADlB,IAAA,QAAM,EAAE,QAAA,CAAA;AACU,GALnB,E,IAAA,E;;AAAA,I,gCAAA,C,CAMI,CAxBN,C","sourcesContent":["<template>\n  <div class=\"container\">\n    <app-alert :alert=\"alert\" @close=\"alert = null\"></app-alert>\n\n    <form class=\"card\" @submit.prevent=\"createPerson\">\n      <h2>Работа с базой данных</h2>\n\n      <div class=\"form-control\">\n        <label for=\"name\">Введите имя</label>\n        <input type=\"text\" id=\"name\" v-model.trim=\"name\">\n      </div>\n\n      <button class=\"btn primary\" :disabled=\"name.length === 0\">\n      Создать человека\n      </button>\n    </form>\n\n    <app-loader v-if=\"loading\"></app-loader>\n\n    <app-people-list\n    v-else \n    :people=\"people\"\n    @load=\"loadPeople\"\n    @remove=\"removePerson\"\n    ></app-people-list>\n  </div>\n</template>\n\n<script>\nimport AppPeopleList from './AppPeopleList.vue'\nimport AppAlert from './AppAlert.vue'\nimport AppLoader from './AppLoader.vue'\nimport axios from 'axios'\n\nexport default {\n  data() {\n    return {\n      name: '',\n      people: [],\n      alert: null,\n      loading: false\n    }\n  },\n  mounted() {\n    this.loadPeople()\n  },\n  methods: {\n    async createPerson() {\n      const response = await fetch('https://vue-with-http-9g-default-rtdb.firebaseio.com/people.json', {\n        method: 'POST',\n        headers: {\n          'Content-Type': 'application/json'\n        },\n        body: JSON.stringify({\n          firstName: this.name\n        })\n      })\n\n      const firebaseData = await response.json()\n\n      this.people.push({\n        firstName: this.name,\n        id: firebaseData.name\n      })\n      this.name = ''\n    },\n\n    loadPeople() {\n        this.loading = true\n        setTimeout(async() => {\n          try{\n            const {data} = await axios.get('https://vue-with-http-9g-default-rtdb.firebaseio.com/people.json')\n            if (!data) {\n              throw new Error('Список людей пуст')\n            }\n          \n            this.people = Object.keys(data).map(key => {\n            return {\n            id: key,\n            ...data[key]\n          }\n        })\n        this.loading = false\n          } catch (e) {\n        this.alert = {\n          type: 'danger',\n          title: 'Ошибка!',\n          text: e.message\n        }\n        this.loading = false\n        console.log(e.message);\n          }\n        }, 1500)\n    },\n    async removePerson(id) {\n      try {\n        const name = this.people.find(person => person.id === id).firstName\n        await axios.delete(`https://vue-with-http-9g-default-rtdb.firebaseio.com/people/${id}.json`)\n        this.people = this.people.filter(person => person.id !== id)\n        this.alert = {\n          type: 'primary',\n          title: 'Успешно!',\n          text: `Пользователь с именем \"${name}\" был удалён!`\n\n        }\n      } catch (e) {\n\n      }\n      \n    }\n  },\n  components: {AppPeopleList, AppAlert,AppLoader}\n}\n</script>\n\n<style>\n\n</style>\n"],"sourceRoot":""}]}