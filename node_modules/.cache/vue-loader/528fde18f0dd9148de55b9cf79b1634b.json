   this.loadPeople\n  },\n  methods: {\n    async createPerson() {\n      const response = await fetch('https://vue-with-http-9g-default-rtdb.firebaseio.com/people.json', {\n        method: 'POST',\n        headers: {\n          'Content-Type': 'application/json'\n        },\n        body: JSON.stringify({\n          firstName: this.name\n        })\n      })\n\n      const firebaseData = await response.json()\n\n      this.people.push({\n        firstName: this.name,\n        id: firebaseData.name\n      })\n      this.name = ''\n    },\n\n    async loadPeople() {\n      const {data} = await axios.get('https://vue-with-http-9g-default-rtdb.firebaseio.com/people.json')\n      this.people = Object.keys(data).map(key => {\n        return {\n          id: key,\n          ...data[key]\n        }\n      })\n    }\n  },\n  components: {AppPeopleList}\n}\n</script>\n\n<style>\n\n</style>\n"]}]}